{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "noEmit": true, // We don't emit b/c esbuild actually builds.
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "outDir": "bin"
  }
}